   ],
   "source": [
    "import scipy.stats as st\n",
    "\n",
    "M = 100\n",
    "sd = 15\n",
    "\n",
    "Xi = 125\n",
    "z = (Xi - M) / sd\n",
    "\n",
    "print(f\"{z=}\")\n",
    "\n",
    "# < Xi\n",
    "cdf = st.norm.cdf(z)\n",
    "print(f\"{cdf=}\")\n",
    "\n",
    "# Какой приблизительно процент людей обладает IQ > 125?\n",
    "moreXi = 1 - cdf\n",
    "print(f\"{cdf=}\")\n"
   ]
  }
